{
  "version": 2,
  "builds": [
    {
      "src": "/api/*.js",
      "use": "@now/node"
    },
    {
      "src": "/www/*.html",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ]
}